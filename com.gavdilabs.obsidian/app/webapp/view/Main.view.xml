<mvc:View controllerName="com.gavdilabs.ui5template.controller.Main" displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

<Page title="Easy Comply">
		<content>
			<TileContainer id="container">
				<StandardTile id="tileGroup" visible="true" title="Group"
					icon="sap-icon://company-view" iconDensityAware="false" press="handleGroupNav" />
				<StandardTile id="tileCountry" visible="true" title="Country" icon="sap-icon://group" iconDensityAware="false"
					press="handleCountryNav" />
			</TileContainer>
		</content>
	</Page>
</mvc:View>
