<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:uitable="sap.ui.table"
    xmlns:uxap="sap.uxap"
    xmlns:unified="sap.ui.unified"
    xmlns:form="sap.ui.layout.form">
        <uxap:ObjectPageSubSection showTitle = "true" title="EU" class="sapUiSmallMarginTop">
            <uxap:blocks>
                <uitable:Table id="euLMStable" visibleRowCount="4" selectionMode="None" ariaLabelledBy="title" items="{dataentries}">
                    <uitable:extension>
                        <OverflowToolbar style="Clear">
                            <Title text="GDPR Complaince Course"/>
                        </OverflowToolbar>
                    </uitable:extension>
                    <uitable:columns>
                        <uitable:Column width="100%">
                            <Label text="Data entry" />
                            <uitable:template>
                                <Text text="{dataentry}" />
                            </uitable:template>
                        </uitable:Column>
                    </uitable:columns>
                </uitable:Table>
            </uxap:blocks>
        </uxap:ObjectPageSubSection>
</core:FragmentDefinition>
